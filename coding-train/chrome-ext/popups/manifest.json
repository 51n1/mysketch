{
  "manifest_version": 2,
  "name": "Pop Up Test",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo5.png",
    "default_popup": "sketch/index.html",
    "default_title": "Generate bubbles!"
  }
}
